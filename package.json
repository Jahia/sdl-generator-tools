    "@jahia/react-material": "^3.0.5",
    "@jahia/ui-extender": "^1.0.4",
    "@material-ui/core": "^3.6.2",
    "@material-ui/icons": "^3.0.1",
    "@material-ui/lab": "^3.0.0-alpha.16",
    "brace": "^0.11.1",
    "fuse.js": "^6.4.1",
    "graphql": "0.12.3",
    "graphql-tag": "^2.9.1",
    "lodash": "^4.17.20",
    "minimist": "^1.2.0",
    "prop-types": "^15.7.2",
    "react": "^16.14.0",
    "react-ace": "^9.1.3",
    "react-apollo": "^3.1.5",
    "react-dom": "^16.14.0",
    "react-i18next": "^11.16.9",
    "react-redux": "^8.0.2",
    "react-select": "^5.3.2",
    "redux": "^4.2.0"
  },
  "devDependencies": {
    "@babel/cli": "^7.6.2",
    "@babel/core": "^7.6.2",
    "@babel/plugin-proposal-class-properties": "^7.5.0",
